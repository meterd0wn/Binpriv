{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nbasez \"$LFILE\" | basez --decode\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./basez \"$LFILE\" | basez --decode\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo basez \"$LFILE\" | basez --decode\n"
      }
    ]
  }
}
