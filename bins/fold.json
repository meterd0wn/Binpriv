{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nfold -w99999999 \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./fold -w99999999 \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo fold -w99999999 \"$LFILE\"\n"
      }
    ]
  }
}
