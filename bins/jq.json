{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\njq -Rr . \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./jq -Rr . \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo jq -Rr . \"$LFILE\"\n"
      }
    ]
  }
}
