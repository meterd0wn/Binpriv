{
  "description": "The read file content is corrupted by replacing tabs with spaces.",
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nexpand \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./expand \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo expand \"$LFILE\"\n"
      }
    ]
  }
}
