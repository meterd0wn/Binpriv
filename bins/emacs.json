{
  "functions": {
    "shell": [
      {
        "code": "emacs -Q -nw --eval '(term \"/bin/sh\")'"
      }
    ],
    "file-write": [
      {
        "code": "emacs file_to_write\nDATA\nC-x C-s\n"
      }
    ],
    "file-read": [
      {
        "code": "emacs file_to_read"
      }
    ],
    "suid": [
      {
        "code": "./emacs -Q -nw --eval '(term \"/bin/sh -p\")'"
      }
    ],
    "sudo": [
      {
        "code": "sudo emacs -Q -nw --eval '(term \"/bin/sh\")'"
      }
    ]
  }
}
