{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nhead -c1G \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./head -c1G \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo head -c1G \"$LFILE\"\n"
      }
    ]
  }
}
