{
  "description": "This is a GUI application. The file content is displayed in a sticky note.",
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nxpad -f \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo xpad -f \"$LFILE\"\n"
      }
    ]
  }
}
