{
  "functions": {
    "file-read": [
      {
        "code": "export LFILE=file_to_read\nzsh -c 'echo \"$(<$LFILE)\"'\n"
      }
    ],
    "file-write": [
      {
        "code": "export LFILE=file_to_write\nzsh -c 'echo DATA >$LFILE'\n"
      }
    ],
    "shell": [
      {
        "code": "zsh"
      }
    ],
    "suid": [
      {
        "code": "./zsh"
      }
    ],
    "sudo": [
      {
        "code": "sudo zsh"
      }
    ]
  }
}
