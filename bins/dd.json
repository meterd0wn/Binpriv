{
  "functions": {
    "file-write": [
      {
        "code": "LFILE=file_to_write\necho \"DATA\" | dd of=$LFILE\n"
      }
    ],
    "file-read": [
      {
        "code": "LFILE=file_to_read\ndd if=$LFILE\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_write\necho \"data\" | ./dd of=$LFILE\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_write\necho \"data\" | sudo dd of=$LFILE\n"
      }
    ]
  }
}
