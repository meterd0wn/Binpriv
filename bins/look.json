{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nlook '' \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./look '' \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo look '' \"$LFILE\"\n"
      }
    ]
  }
}
