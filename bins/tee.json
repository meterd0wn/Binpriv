{
  "description": "It can only append data if the destination exists.",
  "functions": {
    "file-write": [
      {
        "code": "LFILE=file_to_write\necho DATA | ./tee -a \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_write\necho DATA | ./tee -a \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_write\necho DATA | sudo tee -a \"$LFILE\"\n"
      }
    ]
  }
}
