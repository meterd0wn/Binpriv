{
  "description": "`column` expects textual data.\n",
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\ncolumn $LFILE\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./column $LFILE\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo column $LFILE\n"
      }
    ]
  }
}
