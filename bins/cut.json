{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\ncut -d \"\" -f1 \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./cut -d \"\" -f1 \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo cut -d \"\" -f1 \"$LFILE\"\n"
      }
    ]
  }
}
