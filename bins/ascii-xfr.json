{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nascii-xfr -ns \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./ascii-xfr -ns \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo ascii-xfr -ns \"$LFILE\"\n"
      }
    ]
  }
}
