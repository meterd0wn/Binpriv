{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nbasenc --base64 $LFILE | basenc -d --base64\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\nbasenc --base64 $LFILE | basenc -d --base64\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo basenc --base64 $LFILE | basenc -d --base64\n"
      }
    ]
  }
}
