{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nw3m \"$LFILE\" -dump\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./w3m \"$LFILE\" -dump\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo w3m \"$LFILE\" -dump\n"
      }
    ]
  }
}
