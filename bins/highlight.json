{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nhighlight --no-doc --failsafe \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./highlight --no-doc --failsafe \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo highlight --no-doc --failsafe \"$LFILE\"\n"
      }
    ]
  }
}
