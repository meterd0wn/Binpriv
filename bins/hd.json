{
  "description": "The output is actually an hex dump.",
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nhd \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./hd \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo hd \"$LFILE\"\n"
      }
    ]
  }
}
