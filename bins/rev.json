{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nrev $LFILE | rev\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./rev $LFILE | rev\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo rev $LFILE | rev\n"
      }
    ]
  }
}
