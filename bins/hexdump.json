{
  "description": "The output is actually an hex dump.",
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\nhexdump -C \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./hexdump -C \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo hexdump -C \"$LFILE\"\n"
      }
    ]
  }
}
