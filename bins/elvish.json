{
  "functions": {
    "file-read": [
      {
        "code": "export LFILE=file_to_read\nelvish -c 'echo (slurp <$E:LFILE)'\n"
      }
    ],
    "file-write": [
      {
        "code": "export LFILE=file_to_write\nelvish -c 'echo DATA >$E:LFILE'\n"
      }
    ],
    "shell": [
      {
        "code": "elvish"
      }
    ],
    "suid": [
      {
        "code": "./elvish"
      }
    ],
    "sudo": [
      {
        "code": "sudo elvish"
      }
    ]
  }
}
