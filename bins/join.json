{
  "functions": {
    "file-read": [
      {
        "code": "LFILE=file_to_read\njoin -a 2 /dev/null $LFILE\n"
      }
    ],
    "suid": [
      {
        "code": "LFILE=file_to_read\n./join -a 2 /dev/null $LFILE\n"
      }
    ],
    "sudo": [
      {
        "code": "LFILE=file_to_read\nsudo join -a 2 /dev/null $LFILE\n"
      }
    ]
  }
}
